<div>
    <h1>Listado de productos</h1>
      {{#each getProducts}}
       <div>
          <p><b>ID: {{this.id}}</b></p>
          <h2>{{this.title}}</h2>
          <p>{{this.description}}</p>
          <img width="420" src="{{this.thumbnail}}" alt="{{this.title}}">
          <p>Price: {{this.price}}</p>
          <p>Code: {{this.code}}</p>
          <p>Stock: {{this.stock}}</p>
          <p>Category: {{this.category}}</p>
        </div>
      {{/each}}
</div>

<script src="/socket.io/socket.io.js"></script>
<script src="/js/socketIndex.js"></script>